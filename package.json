{
  "name": "toctoc-auth",
  "version": "1.0.8",
  "description": "A simple auth library made for backend meatheads.",
  "keywords": [
    "auth",
    "react",
    "jwt",
    "authentication",
    "authorization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LibardiFelipe/toctoc-auth"
  },
  "license": "MIT",
  "author": "Felipe Libardi Gon√ßalves",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts && copy README.md dist\\",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "crypto-js": "^4.2.0",
    "csstype": "^3.1.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/react": "^19.1.8",
    "copyfiles": "^2.4.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
